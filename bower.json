{
  "name": "cf-typography",
  "version": "0.5.1",
  "description": "Basic typography styling, including heading hierarchy, links, and some handy webfont mixins for Capital Framework.",
  "keywords": ["capital-framework", "capital", "typography", "less"],
  "authors": [
    {
      "name": "Consumer Financial Protection Bureau",
      "url": "http://cfpb.github.io/"
    }
  ],
  "homepage": "https://github.com/cfpb/cf-typography",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/cf-typography.git"
  },
  "license": "https://github.com/cfpb/cf-typography/blob/master/TERMS.md",
  "main": "src/typography.less",
  "dependencies": {
    "fj-colors": "git://fake.ghe.domain/flapjack/fj-colors.git"
  },
  "exportsOverride": {
    "fj-*": {
      "css": "src/*.css",
      "less": "src/*.less",
      "fonts": "src/fonts/*.*"
    },
    "cfpb-font-icons": {
      "css": "src/*.css",
      "fonts": "static/fonts/*.*"
    },
    "font-awesome": {
      "css": "css/*.css",
      "fonts": "font/*.*"
    }
  }
}
